$margins = {
    '5': 5px,
    '10': 10px,
    '15': 15px,
    '20': 20px,
    '25': 25px,
    '30': 30px,
    '35': 35px,
    '40': 40px,
    '45': 45px,
    '50': 50px
}


.m
    for $number, $width in $margins
        &b-{$number}
            margin-bottom $width
            
        &t-{$number}
            margin-top $width
            
        &l-{$number}
            margin-left $width
            
        &r-{$number}
            margin-right $width
